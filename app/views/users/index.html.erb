<div class="container">
  <div class="row">
    <div class="col-sm-3">

<%= render 'temp/temp', user: current_user ,book: @book %>
</div>

    <div class="col-sm-9">
<h2>Users</h2>

<table class="table table-striped">
	<tr>
		<th>image</th>
		<th>name</th>
	</tr>
</table>

<% @users.each do |user| %>
<table class="table table-striped">
    <tr>
        <td><span><%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: "img", fallback: "default.jpg", size: "60x60" %> </span> </td>
    	<td><span><%= user.name %> </span> </td>
    	<td><span><%= link_to "show", user_path(user.id) %> </span> </td>
    </tr>
</table>
<% end %>
</div>
</div>
</div>
